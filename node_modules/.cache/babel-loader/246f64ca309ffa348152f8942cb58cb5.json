{"ast":null,"code":"export default {\n  name: 'CalculatorArea',\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      showCal: '',\n      testString: '' // firstEqual :true\n      // tempOperator:[],\n\n    };\n  },\n\n  methods: {\n    show(val) {\n      if (val.target.value == 'AC') {\n        //all clean\n        this.showCal = '';\n        this.testString = ''; // this.firstEqual = true\n      } else if (val.target.value == 'C') {\n        //15位數\n        this.showCal = '';\n      } else if (val.target.value == '▶️') {\n        //15位數\n        this.showCal = this.showCal.slice(0, -1);\n      } else if (this.showCal.length < 15) {\n        //15位數\n        this.showCal += val.target.value;\n      }\n    },\n\n    equal() {\n      this.testString += this.showCal;\n      console.log('before result: ' + this.testString);\n      this.showCal = eval(this.testString);\n      this.testString = ''; // this.testString = this.showCal\n      // this.firstEqual = false\n\n      console.log('after result: ' + this.showCal);\n    },\n\n    operator(oper) {\n      // if(this.firstEqual==true){   \n      //   this.testString += this.showCal\n      // }\n      this.testString += this.showCal;\n\n      switch (oper.target.value) {\n        case '+':\n          this.testString += '+';\n          break;\n\n        case '-':\n          this.testString += '-';\n          break;\n\n        case 'x':\n          this.testString += '*';\n          break;\n\n        case '÷':\n          this.testString += '/';\n          break;\n      } // this.testString += oper.target.value //\n\n\n      this.showCal = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAuCA,eAAe;EACbA,IAAI,EAAE,gBADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAFM;;EAKbC,IAAI,GAAE;IACJ,OAAM;MACJC,OAAO,EAAC,EADJ;MAEJC,UAAS,EAAG,EAFR,CAGJ;MACA;;IAJI,CAAN;EAQD,CAdY;;EAebC,OAAO,EAAC;IACNC,IAAI,CAACC,GAAD,EAAK;MAEP,IAAGA,GAAG,CAACC,MAAJ,CAAWC,KAAX,IAAoB,IAAvB,EAA4B;QAAE;QAC5B,KAAKN,OAAL,GAAe,EAAf;QACA,KAAKC,UAAL