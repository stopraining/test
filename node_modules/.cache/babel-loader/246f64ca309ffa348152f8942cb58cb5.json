  <input class=\"btn btn-primary m-2\" type=\"button\" value=\"%\"><br>\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"7\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"8\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"9\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"÷\" @click=\"operator\"><br>\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"4\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"5\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"6\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"x\" @click=\"operator\"><br>\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"1\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"2\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"3\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"-\" @click=\"operator\"><br>\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\".\" @click=\"show\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"0\" @click=\"show\">\n            <input class=\"btn btn-secondary m-2\" type=\"button\" value=\"=\" @click=\"equal\">\n            <input class=\"btn btn-primary m-2\" type=\"button\" value=\"+\" @click=\"operator\"><br>\n          </form>\n        </div>\n      </div>\n    </div>\n    \n  </div>\n</template>\n\n<script>\nexport default {\n  name: 'CalculatorArea',\n  props: {\n    msg: String\n  },\n  data(){\n    return{\n      showCal:'',\n      testString :'',\n      // firstEqual :true\n      // tempOperator:[],\n      \n    }\n\n  },\n  methods:{\n    show(val){\n\n      if(val.target.value == 'AC'){ //all clean\n        this.showCal = ''\n        this.testString = ''\n        // this.firstEqual = true\n\n      }else if(val.target.value =='C'){ //15位數\n        this.showCal = ''\n\n      }else if(val.target.value =='▶️'){ //15位數\n        this.showCal = this.showCal.slice(0,-1)\n\n      }else if(this.showCal.length<15){ //15位數\n        this.showCal += val.target.value\n      }\n\n    },\n    equal(){\n      this.testString += this.showCal\n      console.log('before result: '+this.testString)\n      this.showCal = eval(this.testString)\n      this.testString = ''\n      // this.testString = this.showCal\n      // this.firstEqual = false\n      console.log('after result: '+this.showCal)\n\n      \n    },\n    operator(oper){\n      // if(this.firstEqual==true){   \n      //   this.testString += this.showCal\n      // }\n      this.testString += this.showCal\n      switch(oper.target.value){\n        case '+':\n          this.testString += '+'\n          break\n        case '-':   \n          this.testString += '-'\n          break \n        case 'x':\n          this.testString += '*'\n          break\n        case '÷':\n        this.testString += '/'\n        break\n      }\n      // this.testString += oper.target.value //\n      this.showCal = ''\n    \n      \n    }\n  }\n}\n</script>\n\n<!-- Add \"scoped\" attribute to limit CSS to this component only -->\n<style lang=\"scss\" scoped>\n\n\n*{\n  margin: auto;\n  padding: auto;\n}\n\n.main{\n  background-color: lightskyblue;\n  color: black;\n  height: auto;\n  width: 320px;\n  padding: 20px;\n  border-radius: 20px;\n}\n.btn{\n  width: 50px;\n} \n.show{\n  background-color:darkgray;\n  width: 255px;\n  height: 50px;\n  text-align: right;\n  font-size: 20pt;\n  font-family: 'Courier Prime', monospace;\n  padding: 5px;\n}\n\n\nh1{\n  color: cadetblue ;\n}\n\n@import \"~bootstrap/scss/bootstrap\";\n@import url('https://fonts.googleapis.com/css2?family=Courier+Prime&display=swap'); \n</style>\n"]},