{"ast":null,"code":"export default {\n  name: 'CalculatorArea',\n  props: {\n    msg: String\n  },\n\n  data() {\n    return {\n      showCal: '',\n      testString: '',\n      firstEqual: true // tempOperator:[],\n\n    };\n  },\n\n  methods: {\n    show(val) {\n      if (val.target.value == 'AC') {\n        //all clean\n        this.showCal = '';\n        this.testString = '';\n        this.firstEqual = true;\n      } else if (this.showCal.length < 15) {\n        //15位數\n        this.showCal += val.target.value;\n      }\n    },\n\n    equal() {\n      this.testString += this.showCal;\n      console.log('before result: ' + this.testString);\n      this.showCal = eval(this.testString);\n      this.testString = this.showCal;\n      this.firstEqual = false;\n      console.log('after resulat: ' + this.testString);\n    },\n\n    operator(oper) {\n      if (this.firstEqual == true) {\n        this.testString += this.showCal;\n      }\n\n      this.testString += oper.target.value;\n      this.showCal = '';\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,eAAe;EACbA,IAAI,EAAE,gBADO;EAEbC,KAAK,EAAE;IACLC,GAAG,EAAEC;EADA,CAFM;;EAKbC,IAAI,GAAE;IACJ,OAAM;MACJC,OAAO,EAAC,EADJ;MAEJC,UAAS,EAAG,EAFR;MAGJC,UAAS,EAAG,IAHR,CAIJ;;IAJI,CAAN;EAQD,CAdY;;EAebC,OAAO,EAAC;IACNC,IAAI,CAACC,GAAD,EAAK;MAEP,IAAGA,GAAG,CAACC,MAAJ,CAAWC,KAAX,IAAoB,IAAvB,EAA4B;QAAE;QAC5B,KAAKP,OAAL,GAAe,EAAf;QACA,KAAKC,UAAL,GAAkB,EAAlB;QACA,KAAKC,UAAL,GAAkB,IAAlB;MAED,CALD,MAKM,IAAG,KAAKF,OAAL,CAAaQ,MAAb,GAAoB,EAAvB,EAA0B;QAAE;QAChC,KAAKR,OAAL,IAAgBK,GAAG,CAACC,MAAJ,CAAWC,KAA3B;MACF;IAED,CAZK;;IAaNE,KAAK,GAAE;MACL,KAAKR,UAAL,IAAmB,KAAKD,OAAxB;MACAU,OAAO,CAACC,GAAR,CAAY,oBAAkB,KAAKV,UAAnC;MACA,KAAKD,OAAL,GAAeY,IAAI,CAAC,KAAKX,UAAN,CAAnB;MACA,KAAKA,UAAL,GAAkB,KAAKD,OAAvB;MACA,KAAKE,UAAL,GAAkB,KAAlB;MACAQ,OAAO,CAACC,GAAR,CAAY,oBAAkB,KAAKV,UAAnC;IAGD,CAtBK;;IAuBNY,QAAQ,CAACC,IAAD,EAAM;MACZ,IAAG,KAAKZ,UAAL,IAAiB,IAApB,EAAyB;QACvB,KAAKD,UAAL,IAAmB,KAAKD,OAAxB;MACF;;MAEA,KAAKC,UAAL,IAAmBa,IAAI,CAACR,MAAL,CAAYC,K